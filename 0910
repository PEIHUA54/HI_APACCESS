// å–æ˜¯å¦åŒ…å«å­ç¾¤çµ„
private bool GetIsIncludeChild() =>
    cmbChildGroup.SelectedItem?.ToString().Equals("true", StringComparison.OrdinalIgnoreCase) ?? false;

// å– Type (1:é é¢&æŒ‰éˆ•è¨­å®š, 2:æ¬Šé™è¨­å®š)
private string GetTypeValue()
{
    var val = cmbType.SelectedItem?.ToString();
    return val == "1" ? "1" : "2";
}


