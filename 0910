// å– ComboBox å€¼ (TEST / PROD / DEV)
private string GetEnvironment()
{
    var val = cmbEnv.SelectedItem?.ToString();

    if (string.IsNullOrEmpty(val)) 
        return "TEST"; // é è¨­ TEST

    if (val.StartsWith("TEST")) return "TEST";
    if (val.StartsWith("PROD")) return "PROD";
    if (val.StartsWith("DEV"))  return "DEV";

    return "TEST"; // fallback é è¨­
}

