    // ===== 功能2 測試 =====
    // 取得群組人員清單
    public ActionResult Test_GetGroupAllUser()
    {
        var req = new POSFILE_APPLY.Models.M_AP.GetGroupAllUserRequestModel
        {
            SysID = "HI_POSIMG",
            GroupID = "001",
            IsIncludeChildGroups = true,
            Environment = "TEST"
        };

        string json_input = JsonConvert.SerializeObject(req);
        string response = new APACCESS().GetGroupAllUser(json_input);
        var res = JsonConvert.DeserializeObject<POSFILE_APPLY.Models.M_AP.GetGroupAllUserResponseModel>(response);

        return Content($"<pre>{JsonConvert.SerializeObject(res, Formatting.Indented)}</pre>", "text/html");
    }

        // ===== 功能2 =====
        public class GetGroupAllUserRequestModel
        {
            public string SysID { get; set; }
            public string GroupID { get; set; }
            public bool IsIncludeChildGroups { get; set; }
            public string Environment { get; set; }
        }
        public class GetGroupAllUserResponseModel
        {
            public string Result_code { get; set; }
            public string Msg { get; set; }
            public ResultData Result { get; set; }
            public class ResultData
            {
                [JsonProperty("UserIDList")]
                public List<string> UserIDList { get; set; }
            }
        }

